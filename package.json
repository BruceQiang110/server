{
  "name": "muffin",
  "version": "1.2.0",
  "description": "The 21st century way of building websites",
  "main": "dist/index.js",
  "scripts": {
    "start": "gulp",
    "prepublish": "gulp transpile"
  },
  "files": [
    "dist"
  ],
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muffinjs/server.git"
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "keywords": [
    "cms",
    "content",
    "management",
    "system",
    "chocolate"
  ],
  "greenkeeper": {
    "emails": false,
    "ignore": [
      "koa-mount",
      "koa-sendfile"
    ]
  },
  "author": "Leonard Lamprecht <mindrun@icloud.com> (http://leo.im)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/muffinjs/server/issues"
  },
  "homepage": "http://muffin.cafe",
  "dependencies": {
    "bcryptjs": "^2.3.0",
    "chalk": "^1.1.3",
    "fs-extra": "^0.28.0",
    "gridfs-stream": "^1.1.1",
    "koa": "^1.2.0",
    "koa-body": "^1.4.0",
    "koa-compress": "^1.0.9",
    "koa-handlebars": "^0.5.7",
    "koa-jwt": "^1.1.2",
    "koa-mount": "^1.3.0",
    "koa-router": "^5.4.0",
    "koa-sendfile": "^1.1.1",
    "koa-static": "^2.0.0",
    "mongoose": "^4.4.12",
    "query-string": "^4.1.0"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-cached": "^1.1.0"
  }
}
