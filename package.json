{
  "name": "muffin",
  "version": "1.2.2",
  "description": "The 21st century way of building websites",
  "main": "dist/index.js",
  "scripts": {
    "start": "gulp",
    "prepublish": "gulp transpile"
  },
  "files": [
    "dist"
  ],
  "babel": {
    "presets": [
      "es2015"
    ],
