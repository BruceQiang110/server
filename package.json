  "bugs": {
    "url": "https://github.com/muffinjs/server/issues"
  },
  "homepage": "http://muffin.cafe",
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "bcryptjs": "^2.3.0",
    "chalk": "^1.1.3",
    "dotenv": "^2.0.0",
    "fs-extra": "^0.29.0",
    "gridfs-stream": "^1.1.1",
    "handlebars": "^4.0.5",
    "koa": "^2.0.0",
    "koa-body": "^1.4.0",
    "koa-compress": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-jwt": "^1.2.0",
    "koa-mount": "^2.0.0",
    "koa-router": "^7.0.1",
    "koa-static": "^3.0.0",
    "koa-views": "^5.0.1",
