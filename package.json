{
  "name": "muffin",
  "version": "0.1.0",
  "description": "Who doesn't like chocolate?",
  "main": "index.js",
  "private": true,
  "dependencies": {
    "bcryptjs": "^2.3.0",
    "dotenv": "^2.0.0",
    "gridfs-stream": "^1.1.1",
    "kerberos": "0.0.18",
    "koa": "^1.1.2",
    "koa-body": "^1.4.0",
    "koa-compress": "^1.0.8",
    "koa-generic-session": "^1.10.1",
    "koa-handlebars": "^0.5.7",
    "koa-mount": "^1.3.0",
    "koa-router": "^5.4.0",
    "koa-session-mongoose": "^0.5.2",
    "koa-static": "^2.0.0",
    "mongoose": "^4.4.2",
    "nodemailer": "^2.1.0",
    "nodemailer-sendmail-transport": "^1.0.0"
  },
  "devDependencies": {
    "babel-preset-es2015-rollup": "^1.1.1",
    "dropzone": "^4.3.0",
    "gulp-rollup": "^1.6.0",
    "koa-livereload": "^0.1.23",
    "node-sass": "^3.4.2",
    "nodemon": "^1.9.0",
    "open": "0.0.5",
    "rollup": "^0.25.4",
    "rollup-plugin-babel": "^2.3.9",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-node-resolve": "^1.4.0",
    "watch": "^0.17.1"
  },
  "scripts": {
    "install": "npm run build",
    "start": "nodemon index -w views/ -i assets/",
    "build:js": "rollup assets/js/app.js > dist/app.js",
    "build:scss": "node-sass assets/scss -o dist --output-style compressed",
    "build:images": "cp -r assets/images/* dist/images",
    "prebuild": "rm -rf dist && mkdir dist && mkdir dist/images",
    "build": "npm run build:js && npm run build:scss && npm run build:images"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leo/muffin.git"
  },
  "engines": {
    "node": "latest"
  },
